<%- include('../partials/header.ejs', {subPage: false, isLogin: isLogin}) %> 
<div class="container-fluid">
    <div class="d-flex">
        <h1>Browse all SubPages</h1>  
        <a href="/r/all/browse" class="btn btn-primary me-2">Create SubPage</a>
    </div>
</div>
<div class="row row-cols-1 row-cols-md-1 g-4"></div>
    <% for (let i=0; i<allSubs.length; i++) {%> 
        <div class="card mb-3" style="max-width: 1080px;">
            <div class="row g-0">
              <div class="col-md-8">
                <div class="card-body">
                  <a class="card-title text-start" href="/r/<%= allSubs[i].title %>/"><%= allSubs[i].title %></a>
                 </div>
              </div>
            </div>
          </div>
        <% } %> 
    </div>